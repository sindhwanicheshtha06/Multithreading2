class FooBar {
    private int n;
    private boolean printFoo = true;

    public FooBar(int n) {
        this.n = n;
    }

    public void foo(Runnable printFoo) throws InterruptedException {

        for (int i = 0; i < n; i++) {
            synchronized (this) {
                while (!this.printFoo) {
                    wait();
                }
            }
            // printFoo.run() outputs "foo". Do not change or remove this line.
            printFoo.run();
            synchronized (this) {
                this.printFoo = false;
                this.notify();
            }
        }
    }

    public void bar(Runnable printBar) throws InterruptedException {

        for (int i = 0; i < n; i++) {
            synchronized (this) {
                while (this.printFoo) {
                    wait();
                }
            }
            // printBar.run() outputs "bar". Do not change or remove this line.
            printBar.run();
            synchronized (this) {
                this.printFoo = true;
                this.notify();
            }
        }
    }
}
